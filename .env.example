# FedEx Configuration
FEDEX_CLIENT_ID=your_fedex_client_id
FEDEX_CLIENT_SECRET=your_fedex_client_secret
FEDEX_SANDBOX=true

# UPS Configuration  
UPS_CLIENT_ID=your_ups_client_id
UPS_CLIENT_SECRET=your_ups_client_secret
UPS_REDIRECT_URI=http://localhost:8000/callback
UPS_SANDBOX=true

# DHL Configuration
DHL_CLIENT_ID=your_dhl_client_id
DHL_CLIENT_SECRET=your_dhl_client_secret
DHL_SANDBOX=true

# OnTrac Configuration
# Note: Contact ont@ontrac.com with your account number to get API password
ONTRAC_API_KEY=your_ontrac_api_password
ONTRAC_ACCOUNT_NUMBER=your_ontrac_account_number
ONTRAC_SANDBOX=true

# MCP Configuration
MCP_TRANSPORT=stdio
LOG_LEVEL=INFO

# Optional: Custom timeout settings
REQUEST_TIMEOUT=30
TOKEN_REFRESH_BUFFER=60